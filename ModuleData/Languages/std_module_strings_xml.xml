<?xml version="1.0" encoding="utf-8"?>
<base xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" type="string">
  <tags>
    <tag language="English" />
  </tags>
  <strings>
    <string id="PRev0001" text="{=PRev0001}You looted nearby village. Peasants demand to cut someone's head off. What will you say?[rf:idle_angry][ib:closed][if:idle_angry]" />
    <string id="PRev0002" text="{=PRev0002}Just curious, the {PEASANTREVENGER.LINK} says that you looted your own village earlier. Peasant want your head off. What will you say?[if:convo_thinking][if:idle_happy]" />
    <string id="PRev0003" text="{=PRev0003}I'll pay {REPARATION}{GOLD_ICON}." />
    <string id="PRev0004" text="{=PRev0004}I'll not pay." />
    <string id="PRev0005" text="{=PRev0005}I'll not pay to this rat." />
    <string id="PRev0006" text="{=PRev0006}It was all {COMPANION.NAME}'s plan! (lie)" />
    <string id="PRev0007" text="{=PRev0007}Are you sure it was as you say?[if:convo_thinking][ib:closed]" />
    <string id="PRev0008" text="{=PRev0008}No!" />
    <string id="PRev0009" text="{=PRev0009}Yes!" />
    <string id="PRev0010" text="{=PRev0010}Well, maybe it is not for peasant to decide your fate...[if:convo_thinking]" />
    <string id="PRev0011" text="{=PRev0011}Pay for your crime.[rf:idle_angry][if:convo_bored]" />
    <string id="PRev0012" text="{=PRev0012}Well I'm happy with that.[ib:happy]" />
    <string id="PRev0013" text="{=PRev0013}Well, that is unfortunate.[ib:warrior][if:convo_bored][rf:convo_grave]" />
    <string id="PRev0014" text="{=PRev0014}Your prisoner {CRIMINAL.LINK} looted our village. We demand criminal's head on spike! What will you say?[if:convo_furious][ib:agressive]" />
    <string id="PRev0015" text="{=PRev0015}{CRIMINAL.NAME} will die." />
    <string id="PRev0016" text="{=PRev0016}{CRIMINAL.NAME} will pay {REPARATION}{GOLD_ICON}." />
    <string id="PRev0017" text="{=PRev0017}No, it is not your business, peasant!" />
    <string id="PRev0018" text="{=PRev0018}But, but...[ib:closed][if:convo_bared_teeth]" />
    <string id="PRev0019" text="{=PRev0019}Better than nothing...[ib:closed][if:idle_normal]" />
    <string id="PRev0020" text="{=PRev0020}Revenge![if:convo_happy][ib:happy]" />
    <string id="PRev0021" text="{=PRev0021}{PARTYLEADER.LINK} cought {CRIMINAL.LINK} looting our village. We demand criminal's head on spike, because bastard must pay for the crime! What will you say?[ib:aggressive][if:convo_furious]" />
    <string id="PRev0022" text="{=PRev0022}I'll not bow to peasant demands! And {HERO.NAME} should too!" />
    <string id="PRev0023" text="{=PRev0023}I'll not pay! Criminals can die!" />
    <string id="PRev0024" text="{=PRev0024}How dare you! You'll die!" />
    <string id="PRev0025" text="{=PRev0025}Better than nothing...[ib:closed][if:idle_angry][rf:idle_angry]" />
    <string id="PRev0026" text="{=PRev0026}But, but...[if:convo_furious][ib:convo_bared_teeth][ib:aggressive][if:convo_astonished]" />
    <string id="PRev0027" text="{=PRev0027}And I'll send your head to {HERO.NAME}!" />
    <string id="PRev0028" text="{=PRev0028}So, criminal will die.[ib:closed][if:happy]" />
    <string id="PRev0029" text="{=PRev0029}Criminal lord is dead![ib:closed][if:happy]" />
    <string id="PRev0030" text="{=PRev0030}Look, the peasant killed our prisoner![ib:convo_bared_teeth][if:convo_shocked][if:convo_astonished]" />
    <string id="PRev0031" text="{=PRev0031}{?GIFT_RECEIVER.GENDER}Lady{?}Lord{\\?} {GIFT_RECEIVER.LINK} is asking you for ransom gold {RANSOM_COMPENSATION}{GOLD_ICON}.[ib:convo_nervous][if:convo_grave]" />
    <string id="PRev0032" text="{=PRev0032}I'll not pay anything." />
    <string id="PRev0033" text="{=PRev0033}Please, take this gift to {?GIFT_RECEIVER.GENDER}Lady{?}Lord{\\?} {GIFT_RECEIVER.NAME}." />
    <string id="PRev0034" text="{=PRev0034}Your ransom is dead and now you will die too!" />
    <string id="PRev0035" text="{=PRev0035}A gift?[if:convo_thinking]" />
    <string id="PRev0036" text="{=PRev0036}So you cannot pay...[ib:closed][if:idle_angry]" />
    <string id="PRev0037" text="{=PRev0037}We are pleased.[if:convo_happy]" />
    <string id="PRev0038" text="{=PRev0038}It is unfair.[ib:closed][if:idle_angry]" />
    <string id="PRev0039" text="{=PRev0039}But I'm the messenger...[ib:convo_bared_teeth][ib:aggressive][if:convo_astonished]" />
    <string id="PRev0040" text="{=PRev0040}{PARTYOWNER.NAME} did not executed {PRISONER.NAME}, because {SAVER.NAME} paid {REPARATION}{GOLD_ICON}." />
    <string id="PRev0041" text="{=PRev0041}{PARTYOWNER.NAME} did not executed {PRISONER.NAME}, because {PRISONER.NAME} paid {REPARATION}{GOLD_ICON}" />
    <string id="PRev0042" text="{=PRev0042}{PARTYOWNER.NAME} did not executed {PRISONER.NAME}, because different kingdom" />
    <string id="PRev0043" text="{=PRev0043}{PARTYOWNER.NAME} did not executed {PRISONER.NAME}, because {SAVER.NAME} executed peasant notable {EXECUTIONER.NAME}" />
    <string id="PRev0044" text="{=PRev0044}{PARTYOWNER.NAME} did not executed {PRISONER.NAME}, because friends" />
    <string id="PRev0045" text="{=PRev0045}{SAVER.NAME} paid {REPARATION}{GOLD_ICON} for {PRISONER.NAME}'s head to {EXECUTIONER.NAME}." />
    <string id="PRev0046" text="{=PRev0046}{HERO.NAME} paid {REPARATION}{GOLD_ICON} to {EXECUTIONER.NAME}." />
    <string id="PRev0047" text="{=PRev0047}{SETTLEMENTOWNER.NAME} did not executed {PRISONER.NAME}, because friends" />
    <string id="PRev0048" text="{=PRev0048}Do you deal with criminals in this village?" />
    <string id="PRev0049" text="{=PRev0049}Yes, but how can forgiving cowards deter them? With sticks?[ib:closed][if:convo_grave]" />
    <string id="PRev0050" text="{=PRev0050}The problem is in your head!" />
    <string id="PRev0051" text="{=PRev0051}I can try to teach you by my example..." />
    <string id="PRev0052" text="{=PRev0052}Here take {BRIBEVALUE}{GOLD_ICON}. Make criminals pay for their crimes!" />
    <string id="PRev0053" text="{=PRev0053}Thank you very much![if:convo_astonished]" />
    <string id="PRev0054" text="{=PRev0054}I just cannot.[if:convo_grave]" />
    <string id="PRev0055" text="{=PRev0055}Do not have needed traits" />
    <string id="PRev0056" text="{=PRev0056}{SETTLEMENTOWNER.NAME} did not executed {PRISONER.NAME}, because have good relations." />
    <string id="PRev0057" text="{=PRev0057}{SETTLEMENTOWNER.NAME} did not executed {PRISONER.NAME}, because are relatives." />
    <string id="PRev0058" text="{=PRev0058}{PARTYOWNER.NAME} did not executed {PRISONER.NAME}, because have good relations." />
    <string id="PRev0059" text="{=PRev0059}{PARTYOWNER.NAME} did not executed {PRISONER.NAME}, because are relatives." />
    <string id="PRev0060" text="{=PRev0060}Gift to {PROPOSEDTO.NAME}" />
    <string id="PRev0061" text="{=PRev0061}{PROPOSING_HERO.NAME} REPARATION " />
  </strings>
</base>