<?xml version="1.0" encoding="utf-8"?>
<base xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" type="string">
  <tags>
    <tag language="Português (BR)" />
  </tags>
  <strings>
     <string id="PRev0001" text="Você saqueou uma vila próxima. Camponeses exigem cortar a cabeça de alguém. O que você vai dizer?[rf:idle_angry][ib:closed][if:idle_angry]" />
     <string id="PRev0002" text="Só por curiosidade, o {PEASANTREVENGER.LINK} diz que você saqueou sua própria aldeia mais cedo. O camponês quer sua cabeça. O que você vai dizer?[if:convo_thinking][if:idle_happy]" />
     <string id="PRev0003" text="Pagarei {REPARATION}{GOLD_ICON}." />
     <string id="PRev0004" text="Não vou pagar." />
     <string id="PRev0005" text="Não vou pagar para esse rato." />
     <string id="PRev0006" text="Foi tudo plano de {COMPANION.NAME}! (mentira)" />
     <string id="PRev0007" text="Tem certeza de que foi como disse?[if:convo_thinking][ib:closed]" />
     <string id="PRev0008" text="Não!" />
     <string id="PRev0009" text="Sim!" />
     <string id="PRev0010" text="Bem, talvez não seja o camponês a decidir o seu destino...[if:convo_thinking]" />
     <string id="PRev0011" text="Pague pelo seu crime.[rf:idle_angry][if:convo_bored]" />
     <string id="PRev0012" text="Bem, estou feliz com isso.[ib:happy]" />
     <string id="PRev0013" text="Bem, isso é lamentável.[ib:warrior][if:convo_bored][rf:convo_grave]" />
     <string id="PRev0014" text="Seu prisioneiro {CRIMINAL.LINK} saqueou nossa vila. Exigimos a cabeça do criminoso em espeto! O que você diria?[if:convo_furious][ib:aggressive]" />
     <string id="PRev0015" text="{CRIMINAL.NAME} morrerá." />
     <string id="PRev0016" text="{CRIMINAL.NAME} pagará {REPARATION}{GOLD_ICON}." />
     <string id="PRev0017" text="Não, não é da sua conta, camponês!" />
     <string id="PRev0018" text="Mas, mas...[ib:closed][if:convo_bared_teeth]" />
     <string id="PRev0019" text="Melhor que nada...[ib:closed][if:idle_normal]" />
     <string id="PRev0020" text="Vingança![if:convo_happy][ib:happy]" />
     <string id="PRev0021" text="{PARTYLEADER.LINK} pegou {CRIMINAL.LINK} saqueando nossa aldeia. Exigimos a cabeça do criminoso na estaca, porque o bastardo deve pagar pelo crime! O que você vai dizer?[ib:aggressive][if:convo_furious]" />
     <string id="PRev0022" text="Não vou me curvar às demandas dos camponeses! E {HERO.NAME} também deveria!" />
     <string id="PRev0023" text="Eu não vou pagar! O criminoso {CVICTIM.NAME} pode morrer!" />
     <string id="PRev0024" text="Como você ousa! Você vai morrer!" />
     <string id="PRev0025" text="Melhor que nada...[ib:closed][if:idle_angry][rf:idle_angry]" />
     <string id="PRev0026" text="Mas, mas...[if:convo_furious][ib:convo_bared_teeth][ib:aggressive][if:convo_astonished]" />
     <string id="PRev0027" text="E enviarei sua cabeça para {HERO.NAME}!" />
     <string id="PRev0028" text="Então, o criminoso vai morrer.[ib:closed][if:happy]" />
     <string id="PRev0029" text="O senhor do crime está morto![ib:closed][if:happy]" />
     <string id="PRev0030" text="Olha, o camponês matou nosso prisioneiro![ib:convo_bared_teeth][if:convo_shocked][if:convo_astonished]" />
     <string id="PRev0031" text="{?GIFT_RECEIVER.GENDER}Senhora{?}Senhor{\\?} {GIFT_RECEIVER.LINK} está pedindo ouro de resgate {RANSOM_COMPENSATION}{GOLD_ICON}.[ib:convo_nervous][if:convo_grave]" />
     <string id="PRev0032" text="Não pagarei nada." />
     <string id="PRev0033" text="Por favor, leve este presente para {GIFT_RECEIVER.NAME}." />
     <string id="PRev0034" text="Seu resgate está morto e agora você também morrerá!" />
     <string id="PRev0035" text="Um presente?[if:convo_thinking]" />
     <string id="PRev0036" text="Então você não pode pagar...[ib:closed][if:idle_angry]" />
     <string id="PRev0037" text="Estamos satisfeitos.[if:convo_happy]" />
     <string id="PRev0038" text="É injusto.[ib:closed][if:idle_angry]" />
     <string id="PRev0039" text="Mas eu sou o mensageiro...[ib:convo_bared_teeth][ib:aggressive][if:convo_astonished]" />
	   <string id="PRev0040" text="{PARTYOWNER.NAME} não executou {PRISONER.NAME}, porque {SAVER.NAME} pagou {REPARATION}{GOLD_ICON}." />
     <string id="PRev0041" text="{PARTYOWNER.NAME} não executou {PRISONER.NAME}, porque {PRISONER.NAME} pagou {REPARATION}{GOLD_ICON}" />
     <string id="PRev0042" text="{PARTYOWNER.NAME} não executou {PRISONER.NAME}, porque reino diferente" />
     <string id="PRev0043" text="{PARTYOWNER.NAME} não executou {PRISONER.NAME}, porque {SAVER.NAME} executou o notável camponês {EXECUTIONER.NAME}" />
     <string id="PRev0044" text="{PARTYOWNER.NAME} não executou {PRISONER.NAME}, porque amigos" />
     <string id="PRev0045" text="{SAVER.NAME} pagou {REPARATION}{GOLD_ICON} pela cabeça de {PRISONER.NAME} para {EXECUTIONER.NAME}." />
     <string id="PRev0046" text="{HERO.NAME} pagou {REPARATION}{GOLD_ICON} para {EXECUTIONER.NAME}." />
     <string id="PRev0047" text="{SETTLEMENTOWNER.NAME} não executou {PRISONER.NAME}, porque amigos" />
    <string id="PRev0048" text="Você lida com criminosos nesta aldeia?" />
    <string id="PRev0049" text="Não, não me importo.[ib:closed]" />
    <string id="PRev0050" text="Suas intenções criminosas são bem conhecidas por mim." />
    <string id="PRev0051" text="Posso tentar ensiná-lo com meu exemplo..." />
    <string id="PRev0052" text="Tome {BRIBEVALUE}{GOLD_ICON}. Faça os criminosos pagarem por seus crimes!" />
    <string id="PRev0053" text="Muito obrigado![if:convo_astonished]" />
    <string id="PRev0054" text="Eles farão o que quiserem. Não é da minha conta interferir.[ib:closed][if:angry]" />
    <string id="PRev0055" text="Não possui características necessárias" />
    <string id="PRev0056" text="{SETTLEMENTOWNER.NAME} não executou {PRISONER.NAME}, pois possui boas relações." />
    <string id="PRev0057" text="{SETTLEMENTOWNER.NAME} não executou {PRISONER.NAME}, pois são parentes." />
    <string id="PRev0058" text="{PARTYOWNER.NAME} não executou {PRISONER.NAME}, pois possui boas relações." />
    <string id="PRev0059" text="{PARTYOWNER.NAME} não executou {PRISONER.NAME}, porque são parentes." />
    <string id="PRev0060" text="Presente para {PROPOSEDTO.NAME}" />
    <string id="PRev0061" text="{PROPOSING_HERO.NAME} REPARAÇÃO " />
    <string id="PRev0062" text="E se eu não pagar?" />
    <string id="PRev0063" text="O camponês terá sua cabeça.[if:convo_thinking][rf:convo_grave][ib:closed]" />
    <string id="PRev0064" text="Você vai ficar bem.[if:convo_happy][if:convo_thinking][ib:closed]" />
    <string id="PRev0065" text="Tenho amigos que pagarão a reparação." />
    <string id="PRev0066" text="{SAVER.NAME} apoiará você." />
    <string id="PRev0067" text="{SUPPORTERCOUNT} heróis podem apoiá-lo." />
    <string id="PRev0068" text="{PARTYLEADER.LINK} pegou {CRIMINAL.LINK} saqueando nosso vilarejo e {CVICTIM.LINK} foi acusado de planejar tudo isso. Exigimos justiça![ib:aggressive][if:convo_furious]" />
    <string id="PRev0069" text="{CRIMINAL.NAME} acusou {CRIMINALBLAMED.NAME} de saquear a aldeia." />
    <string id="PRev0070" text="Ambos são criminosos!" />
    <string id="PRev0071" text="O que o criminoso diz sobre isso?" />
    <string id="PRev0072" text="Não dê ouvidos a esse canalha.[rf:convo_angry][ib:closed]" />
    <string id="PRev0073" text="Posso jurar! Foi tudo plano de {CVICTIM.LINK}![rf:convo_grave][ib:closed]" />
    <string id="PRev0074" text="{CVICTIM.LINK} é o criminoso.[rf:convo_angry][ib:closed]" />
    <string id="PRev0075" text="Acredito em você." />
    <string id="PRev0076" text="Você está mentindo." />
    <string id="PRev0077" text="Vocês dois merecem vingança camponesa!" />
    <string id="PRev0078" text="Não tenho tempo para conversar.[rf:idle_angry][ib:closed][if:idle_angry]" />
    <string id="PRev0079" text="Deixe-me dizer como você falará comigo sobre seus problemas." />
    <string id="PRev0080" text="Quais problemas?[if:convo_thinking]" />
    <string id="PRev0081" text="Você não deve me interromper imediatamente com qualquer assunto seu." />
    <string id="PRev0082" text="Você deveria me interromper imediatamente com qualquer assunto seu." />
    <string id="PRev0083" text="Nevermind." />
    <string id="PRev0084" text="Sim, minha {?MAINHERO.GENDER}Senhora{?}Senhor{\\?}.[rf:idle_happy]" />
    <string id="PRev0085" text="Revenger" />
    <string id="PRev0086" text="Você decidiu..." />
    <string id="PRev0087" text="Declarar guerra a {KINGDOM} e ajudar {DEFENDER}." />
    <string id="PRev0088" text="Desativar grupo móvel de camponeses notáveis" />
    <string id="PRev0089" text="Ativar partido móvel camponês notável" />
    <string id="PRev0090" text="Desabilite a opção de defender a vila contra grupos móveis neutros" />
    <string id="PRev0091" text="Ativar a opção de defender a vila contra grupos móveis neutros" />
    <string id="PRev0092" text="Vou defender aldeias contra qualquer saqueador." />
    <string id="PRev0093" text="Vou defender aldeias apenas contra meus inimigos." />
    <string id="PRev0094" text="Preciso sair agora." />
    <string id="PRev0095" text="Há algo que gostaria de discutir." />
    <string id="PRev0096" text="{RANSOMER.LINK} oferece a você {GOLD_AMOUNT}{GOLD_ICON} como resgate se você entregar os restos mortais de {CAPTIVE_HERO.NAME}." />
    <string id="PRev0097" text="Ninguém quer pagar pelos restos de {CAPTIVE_HERO.NAME}" />
    <string id="PRev0098" text="Não posso tomar a decisão..." />
    <string id="PRev0099" text="Uma boa decisão...[ib:happy][if:idle_normal]" />
    <string id="PRev0100" text="Não posso tomar a decisão...[if:convo_thinking][ib:closed]" />
    <string id="PRev0101" text="Assim seja[rf:convo_angry][ib:closed]" />
    <string id="PRev0102" text="Uma boa decisão..." />
    <string id="PRev0103" text="{PARTYOWNER.NAME} não executou {PRISONER.NAME}, devido a acusações conflitantes." />
    <string id="PRev0104" text="{SETTLEMENTOWNER.NAME} não executou {PRISONER.NAME}, devido a acusações conflitantes." />
    <string id="PRev0105" text="Vou levar os restos mortais." />
    <string id="PRev0106" text="..." />
    <string id="PRev0107" text="Você deveria abandonar sua vingança, ou então..." />
    <string id="PRev0108" text="O que mais?[rf:idle_angry][ib:closed][if:idle_angry]" />
    <string id="PRev0109" text="Vou cortar sua cabeça!" />
    <string id="PRev0110" text="Volte para sua aldeia!" />
    <string id="PRev0111" text="{CRIMINAL.NAME} e {CVICTIM.LINK} estão mortos! Minha vingança está concluída![if:happy]" />
    <string id="PRev0112" text="Terminei a vingança em {CVICTIM.LINK}![if:happy]" />
    <string id="PRev0113" text="{CRIMINAL.LINK} pagará {GOLD_ICON} ou morrerá![rf:idle_angry][ib:angry]" />
    <string id="PRev0114" text="Ninguém pode impedir minha vingança contra {CRIMINAL.LINK}![rf:idle_angry][if:convo_furious][ib:angry]" />
    <string id="PRev0115" text="Seu grupo é muito pequeno" />
    <string id="PRev0116" text="{CRIMINAL.LINK} morrerá![if:convo_furious][ib:angry]" />
    <string id="PRev0117" text="Chega, não vou mudar minhas intenções![ib:closed][if:convo_bared_teeth][if:idle_angry]" />
    <string id="PRev0118" text="Aqui, pegue {BRIBEVALUE}{GOLD_ICON}. Pessoas nobres não são os criminosos para você." />
    <string id="PRev0119" text="Agora não." />
    <string id="PRev0120" text="Eles estão infringindo a lei e enfrentarão consequências por seus crimes.[ib:closed][if:angry]" />
    <string id="PRev0121" text="Não posso fazer nenhuma promessa..[ib:closed]" />
    <string id="PRev0122" text="O suborno ainda não funcionará." />
    <string id="PRev0123" text="Não é possível subornar. As características do camponês não permitem." />
    <string id="PRev0124" text="Sim. Por que você pergunta?[ib:closed][rf:convo_thinking]" />
    <string id="PRev0125" text="Eu admito.[if:convo_thinking][if:convo_grave]" />
    <string id="PRev0126" text="{EXECUTIONER.LINK} vai cortar sua cabeça!" />
    <string id="PRev0127" text="A justiça exige que você pague pelos seus crimes." />
    <string id="PRev0128" text="Você está certo." />
    <string id="PRev0129" text="O que há para discutir?" />
    <string id="PRev0130" text="Talvez..." />
    <string id="PRev0131" text="Eu acho...[ib:thinking]" />
    <string id="PRev0132" text="Ninguém deveria ter medo desses criminosos." />
    <string id="PRev0133" text="Alguém deve ser responsabilizado pela destruição de nossas aldeias." />
    <string id="PRev0134" text="Faça justiça com suas próprias mãos." />
    <string id="PRev0135" text="Esses criminosos são muito perigosos." />
    <string id="PRev0136" text="Ter pena de um inimigo é encontrar um amigo." />
    <string id="PRev0137" text="Entregue a justiça nas mãos dos nobres. Você não é tão importante." />
    <string id="PRev0138" text="Todo mundo já ouviu seus discursos hostis contra pessoas nobres." />
    <string id="PRev0139" text="Sua bondade para com o inimigo é criminosa." />
    <string id="PRev0140" text="Todo mundo sabe que estou falando a verdade." />
    <string id="PRev0141" text="Sua acusação é infundada.[ib:nervous][if:convo_astonished]"/>
    <string id="PRev0142" text="Não espere que eu mude meus hábitos." />
    <string id="PRev0143" text="Digamos que foi um mal-entendido." />
    <string id="PRev0144" text="Ótima pergunta! Deixe-me mostrar meu exemplo..." />
    <string id="PRev0145" text="Para tentar persuadir o camponês" />
    <string id="PRev0146" text="Expulsar o camponês" />
    <string id="PRev0147" text="Expulsar ou executar o camponês" />
  </strings>
</base>